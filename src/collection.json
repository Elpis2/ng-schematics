{
  "$schema": "../node_modules/@angular-devkit/schematics/collection-schema.json",
  "schematics": {
    "class": {
      "description": "Regrid custom class.",
      "factory": "./class/index#regridClass",
      "schema": "./class/schema.json"
    },
    "component": {
      "description": "Regrid custom component.",
      "factory": "./component/index#regridComponent",
      "schema": "./component/schema.json"
    },
    "directive": {
      "description": "Regrid custom directive.",
